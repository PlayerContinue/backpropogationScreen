<Window x:Class="VisualBackPropogation.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:customgraph="clr-namespace:VisualBackPropogation"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <DockPanel Width="Auto" Height="Auto">
            <Menu IsMainMenu="True" Name="MainMenu"  DockPanel.Dock="Top">
                <MenuItem Header="_File">
                    <MenuItem Header="_Open"/>
                    <MenuItem Click="MenuItem_Click" Header="_Start"/>
                </MenuItem>
                <MenuItem Header="_Display">
                    <MenuItem Header="_GraphView"/>
                </MenuItem>
            </Menu>
        </DockPanel>
        <Frame x:Name="_mainFrame" Margin="0,24,0,0"></Frame>
    </Grid>
</Window>
